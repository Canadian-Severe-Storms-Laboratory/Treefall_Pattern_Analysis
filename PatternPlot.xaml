<UserControl x:Class="TreefallPatternAnalysis.PatternPlot"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TreefallPatternAnalysis"
             mc:Ignorable="d" 
             d:DesignHeight="400" d:DesignWidth="157">
    <Grid>
        <WpfPlot x:Name="patternPlot" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" PreviewMouseDown="OverrideMiddleMouse" Loaded="patternPlot_Loaded"/>
    </Grid>
</UserControl>
